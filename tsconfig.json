{
  "compilerOptions": {
  "target": "ES2020",
  "module": "ES2020",
  "moduleResolution": "node",
  "declaration": true,
  "declarationDir": "dist",
  "types": ["vitest/globals", "node"],
  "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "src/test"]
}
