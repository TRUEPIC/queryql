{
  "name": "@truepic/queryql",
  "version": "0.20.0",
  "description": "Easily add filtering, sorting, and pagination to your REST API through your old friend: the query string!",
  "keywords": [
    "filter",
    "filtering",
    "sort",
    "sorting",
    "page",
    "pagination",
    "rest",
    "api",
    "query",
    "querystring",
    "qs",
    "knex",
    "orm"
  ],
  "homepage": "https://github.com/TRUEPIC/queryql#readme",
  "bugs": "https://github.com/TRUEPIC/queryql/issues",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TRUEPIC/queryql.git"
  },
  "files": [
    "dist",
    "!dist/**/*.test.js",
    "DOCS.md"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "npm run lint:format && npm run lint:quality",
    "lint:format": "prettier --check .",
    "lint:format:fix": "prettier --write .",
    "lint:quality": "eslint src",
    "lint:quality:fix": "eslint --fix src",
    "release": "release-it --only-version",
    "test": "vitest run",
    "test:watch": "vitest",
    "ts": "tsc",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run ts && npm pack --silent",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "is": "^3.3.2",
    "joi": "^18.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/is": "^0.0.25",
    "@types/node": "^24.3.0",
    "@vitest/eslint-plugin": "1.3.4",
    "eslint": "^9.33.0",
    "eslint-plugin-n": "^17.21.3",
    "globals": "^16.3.0",
    "knex": "^3.1.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "sqlite3": "^5.1.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  }
}
